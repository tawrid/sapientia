{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "env": {
      "OPENCAGE_API_KEY": "@opencage_api_key",
      "IPINFO_TOKEN": "@ipinfo_token",
      "SPECIAL_ID": "@special_id"
    },
    "functions": {
      "api/**/*.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }
  