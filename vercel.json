{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ],
    "env": {
      "OPENCAGE_API_KEY": "@opencage_api_key",
      "IPINFO_TOKEN": "@ipinfo_token",
      "SPECIAL_ID": "@special_id"
    }
  }
  